{
  "name": "@netlify/plugin-lighthouse",
  "version": "1.0.0",
  "description": "Netlify Plugin to run Lighthouse on each build",
  "main": "src/index.js",
  "scripts": {
    "local": "node -e 'require(\"./src/index.js\").onSuccess()'",
    "lint": "eslint 'src/**/*.js'",
    "format": "prettier --write 'src/**/*.js'",
    "format:ci": "prettier --check 'src/**/*.js'",
    "tag": "git tag \"v$npm_package_version\" && git push --tags",
    "postpublish": "npm run tag"
  },
  "keywords": [
    "netlify",
    "netlify-plugin",
    "lighthouse"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "chrome-launcher": "^0.13.2",
    "dotenv": "^8.2.0",
    "http-server": "^0.12.3",
    "lighthouse": "^6.0.0",
    "puppeteer": "^3.1.0"
  },
  "engines": {
    "node": ">=v10.21.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "eslint": "^7.0.0",
    "husky": "^4.0.7",
    "prettier": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
